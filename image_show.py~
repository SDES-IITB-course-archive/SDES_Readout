import cv2
image_array=[]
image_array.append(cv2.imread("1.jpg"))
image_array.append(cv2.imread("2.jpg"))
image_array.append(cv2.imread("3.jpg"))
image_array.append(cv2.imread("4.jpg"))

cv2.imshow("1",image_array[0])
cv2.imshow("2",image_array[1])
cv2.imshow("3",image_array[2])
cv2.imshow("4",image_array[3])

cv2.waitKey(0)
